{
  "name": "emeahomechallenge",
  "version": "1.0.0",
  "description": "Experimental App",
  "scripts": {
    "test": "REACT_APP_HOST_URL=http://localhost:5500 jest --env=jsdom --runInBand",
    "start": "parcel ./src/index.html",
    "bootServer": "node ./server/index.js",
    "build": "parcel build ./src/index.html",
    "lint": "eslint './**/*.js' --fix"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "testEnvironment": "node",
    "setupFiles": [
      "jest-prop-type-error"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts|test/e2e-tests)[/\\\\]"
    ],
    "testRegex": "\\.(test|it)\\.jsx?$",
    "testURL": "http://localhost:5500",
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!@material-ui/core)",
      "<rootDir>/node_modules/(?!@material-ui/core/Grid.d.ts)",
      "<rootDir>/node_modules/(?!@babel/runtime)"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.(ts|tsx)?$": "ts-jest",
      "^.+\\.(js|jsx)$": "babel-jest"
    }
  },
  "keywords": [],
  "author": "",
  "license": "proprietary",
  "dependencies": {
    "@material-ui/icons": "^4.9.1",
    "@types/react": "^16.8.6",
    "axios": "^0.24.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "csvtojson": "^2.0.10",
    "express": "^4.17.1",
    "history": "^5.1.0",
    "immer": "^9.0.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.0.2",
    "redux": "^4.1.2",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.0",
    "src": "^1.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.8.3",
    "@material-ui/core": "^4.12.3",
    "@testing-library/dom": "^8.11.1",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^27.3.1",
    "babel-plugin-import": "^1.13.0",
    "babel-register": "^6.26.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-recommended": "^4.0.0",
    "eslint-plugin-compat": "^3.5.1",
    "eslint-plugin-jest": "^23.7.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.3.0",
    "jest": "^27.3.1",
    "jest-prop-type-error": "^1.1.0",
    "nock": "^13.2.1",
    "parcel-bundler": "^1.12.4",
    "prettier": "1.19.1",
    "redux-mock-store": "^1.5.4",
    "ts-jest": "^27.0.7",
    "wait-for-expect": "^3.0.2"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ]
}
